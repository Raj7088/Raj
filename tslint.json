{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-immutable"
  ],
  "rulesDirectory": ["node_modules/vrsource-tslint-rules/rules"],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [true, "log"],
    "max-params": [true, 3]
  },
  "defaultSeverity": "warning"
}
